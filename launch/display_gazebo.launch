<?xml version="1.0"?>
<launch>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
  </include>
    <arg name="model" default="$(find spotmini)/urdf/spotrobot.urdf" />

  <param name="robot_description" command="cat $(find spotmini)/urdf/spotrobot.urdf" />
  
  <node name="spawn_robot" pkg="gazebo_ros" type="spawn_model"
    args="-urdf -model spotmini -param robot_description -z 1">
  </node>

</launch>